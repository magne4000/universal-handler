{
  "name": "@universal-middleware/express",
  "version": "0.0.2",
  "type": "module",
  "description": "Express adapter for universal middlewares",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./request": "./dist/request.js",
    "./response": "./dist/response.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts"
      ]
    }
  },
  "author": "Joël Charles <joel.charles91@gmail.com>",
  "contributors": [
    "Fatih Aygün <cyco130@gmail.com>"
  ],
  "repository": "https://github.com/magne4000/universal-handler",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsup",
    "dev": "tsup --watch",
    "prepack": "pnpm build",
    "test": "pnpm test:typecheck",
    "test:typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "rimraf": "^6.0.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3"
  }
}
